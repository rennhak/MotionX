<?xml version="1.0"?>
<xs:schema targetNamespace="http://foo.example.com/tns"
 xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:tns="http://foo.example.com/tns">

  <!--  -->
  <xs:complexType name="FrameComplexType">
    <!-- @note { e.g. "Thu Jul  2 00:54:22 JST 2009" } -->
    <xs:attribute name="timestamp" type="xs:string" use="optional" default=""""/>
    <!-- @note { e.g. 1246463811 } -->
    <xs:attribute name="epoch" type="xs:unsigned int" use="optional" default="0"/>
  </xs:complexType>

  <!-- Frame is the root element, declared here. -->
  <xs:element name="Frame" type="tns:FrameComplexType"/>
</xs:schema>
